<Prefab>
  <Variables>
  </Variables>
  <VisualDefinitions>
  </VisualDefinitions>
  <Window>
    <BattleMiniMap_MapWidget Brush.GlobalAlphaFactor="@AlphaFactor" IsVisible="@IsEnabled">
      <Children>
        <Widget DataSource="{DeadAgentMarkerViewModels}" WidthSizePolicy="StretchToParent" HeightSizePolicy="StretchToParent" HorizontalAlignment="Left" VerticalAlignment="Top">
          <ItemTemplate>
            <BattleMiniMap_AgentMarkerWidget Brush.GlobalAlphaFactor="@AlphaFactor" BasePosition="@Position" AgentMarkerType="@AgentMarkerType" Brush.GlobalColor="@Color" />
          </ItemTemplate>
        </Widget>
        <Widget DataSource="{AgentMarkerViewModels}" WidthSizePolicy="StretchToParent" HeightSizePolicy="StretchToParent">
          <ItemTemplate>
            <BattleMiniMap_AgentMarkerWidget Brush.GlobalAlphaFactor="@AlphaFactor" BasePosition="@Position" AgentMarkerType="@AgentMarkerType" Brush.GlobalColor="@Color" />
          </ItemTemplate>
        </Widget>
        <BattleMiniMap_CameraMarkerWidget Brush.GlobalAlphaFactor="@AlphaFactor" DataSource="{CameraMarkerLeft}" RotateAngleInRadians="@RotateAngleInRadians" RotateCenter="@RotateCenter" BasePosition="@BasePosition" />
        <BattleMiniMap_CameraMarkerWidget Brush.GlobalAlphaFactor="@AlphaFactor" DataSource="{CameraMarkerRight}" RotateAngleInRadians="@RotateAngleInRadians" RotateCenter="@RotateCenter" BasePosition="@BasePosition" />
        <BattleMiniMap_BoundaryWidget Brush.GlobalAlphaFactor="@AlphaFactor" />
      </Children>
    </BattleMiniMap_MapWidget>
  </Window>
</Prefab>